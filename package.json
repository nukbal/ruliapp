    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?|ts?)$",
    "setupFilesAfterEnv": [
      "./node_modules/react-native-gesture-handler/jestSetup.js",
      "<rootDir>/app/setupTest.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "node"
