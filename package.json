  "jest": {
    "transform": {
      "^.+\\.(tsx?|ts?)$": "ts-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^react-art$": "node-noop"
    }
  }
