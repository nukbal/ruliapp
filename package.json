    "test:watch": "jest --watch --verbose false",
    "test": "jest",
    "lint": "eslint \"app/**\"",
    "pod": "cd ios && pod install",
    "xcode": "open ios/ruliapp.xcworkspace",
    "postinstall": "jetify"
